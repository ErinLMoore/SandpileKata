__lua__
-- sandpiles kata
-- by erin

function test(title,f)
local desc=function(msg,f)
 printh('⚡:desc:'..msg)
 f()
end
local it=function(msg,f)
 printh('⚡:it:'..msg)
 local xs={f()}
 for i=1,#xs do
  if xs[i] == true then
   printh('⚡:assert:true')
  else
   printh('⚡:assert:false')
  end
 end
 printh('⚡:it_end')
end
printh('⚡:test:'..title)
f(desc,it)
printh('⚡:test_end')
end
-- ---------------------------------------------------------------
t = 0

function _update()
 t += 1
end

function _draw()
 cls()
end
-- ------------------------------------------------------------

local sandpile={
  add_sand =
    function(a)
      if a < 3 then return a+1
      else return 0 end
    end,

  create_sandpile =
    function(s)
      rt = {}
      it = {}
      for i=1,s do it[i] = 0 end
      for i=1,s do rt[i] = it end
      return rt
    end
}
-- ------------------------------------------------------------

test('sandpile', function(desc,it)
  desc('sandpile.add_sand()', function()
    local add_sand = sandpile.add_sand

    it('should return the number + one if under 4', function()
      return add_sand(2) == 3
    end)
    it('should return the 0 if 4 or over', function()
      return add_sand(3) == 0
    end)
  end)

  desc('sandpile.create_sandpile()', function()
    local create_sandpile = sandpile.create_sandpile

    it('should return a 2x2 table given 2', function()
      et = { }
      add(et, {0,0})
      add(et, {0,0})
      at  = create_sandpile(2)
      return (#at == #et) and (#at[1] == #et[1])
    end)

    it('should return a 3x3 table given 3', function()
      et = { }
      add(et, {0,0,0})
      add(et, {0,0,0})
      add(et, {0,0,0})
      at  = create_sandpile(3)
      return (#at == #et) and (#at[1] == #et[1])
    end)
  end)
end)



